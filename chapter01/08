def challenge8 (string):
    result = ''

    for i in range(0, len(string)):
        if string[i].islower():
            result += chr(219-ord(string[i]))
        else:
            result += string[i]

    return result

print(challenge8("I Have A Apple"))     # 引数に渡された文字列を文字コードに変換
print(challenge8("I Hzev A Akkov"))     # 文字コードを文字列に変換

